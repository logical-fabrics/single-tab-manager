{
  "manifest_version": 3,
  "name": "Single Tab Manager",
  "version": "1.0.2",
  "description": "同じURLパターンに一致するタブを1つだけに制御する拡張機能",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
